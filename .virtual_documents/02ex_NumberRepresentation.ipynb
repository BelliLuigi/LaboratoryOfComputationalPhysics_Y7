def word2sfp(word):
    word = str(word)
    sign = word[0]
    exp = "0b" + word[1:9]
    mantissa = word[9::]
    dec_mantissa= 1
    e = -1
    for i in mantissa:
        if int(i) == 1: 
            cif = int(i)*2**e
            dec_mantissa += cif
        e -=1
    exp = int(exp,2)
    sign = int(sign)
    number =  (-1)**sign*dec_mantissa*2**(exp-127)
    return number





